{
    "name": "tbcd/data-encryption",
    "type": "library",
    "description": "A library to manage data encryption and decryption",
    "authors": [
        {
            "name": "Thomas Beauchataud"
        }
    ],
    "license": "MIT",
    "keywords": [
        "encryption",
        "decryption"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "version": "1.1.0",
    "require": {
        "php": ">=8.1",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0"
    },
    "autoload": {
        "psr-4": {
            "TBCD\\DataEncryption\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TBCD\\DataEncryption\\Tests\\": "tests/"
        }
    }
}
